
import java.io.*;
import java.util.*;
public class sum3{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int a,b,addition;
        System.out.println("Enter two integers for addition :");
           a= sc.nextInt();
           b= sc.nextInt();
          addition =a+b;
        System.out.println("Sum of"+a+"and"+b+ "is ="+addition);
        }
    }
